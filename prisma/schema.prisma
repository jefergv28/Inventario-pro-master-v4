generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Producto {
  id                Int      @id @default(autoincrement())
  nombre            String
  descripcion       String?
  precio            Float
  cantidad          Int
  creadoEn          DateTime @default(now())
}
